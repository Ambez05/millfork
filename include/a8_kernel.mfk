
#if not(ATARI_8)
#warn a8_kernel module should be used only on Atari computer-compatible targets
#endif

noinline asm void putchar(byte a) {
    ? tax
    lda $347
    pha
    lda $346
    pha
    ? txa
    rts
}

noinline asm byte readkey() {
    lda #$C
    sta $2A
    lda $E425
    pha
    lda $E424
    pha
    rts
}

inline void new_line() {
    putchar($9b)
}
